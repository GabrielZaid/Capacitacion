{
  "kind": "collectionType",
  "collectionName": "menu_diarios",
  "info": {
    "singularName": "menu-diario",
    "pluralName": "menu-diarios",
    "displayName": "MenuDiario"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dia": {
      "type": "string",
      "minLength": 2,
      "maxLength": 150,
      "required": true
    },
    "Primero": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plato.plato"
    },
    "Segundo": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plato.plato"
    },
    "Postre": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plato.plato"
    },
    "Precio": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "Sum_Precio": {
      "type": "decimal",
      "default": 0
    },
    "TipoMenu": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Desayuno",
        "Almuerzo",
        "Comida",
        "Merienda",
        "Cena"
      ]
    }
  }
}
